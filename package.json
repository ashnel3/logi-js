{
  "name": "logi-js",
  "version": "1.0.0",
  "description": "Node bindings for Logitech SDKs",
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./gkey": {
      "import": "./dist/gkey.js",
      "require": "./dist/gkey.cjs"
    },
    "./lcd": {
      "import": "./dist/lcd.js",
      "require": "./dist/lcd.cjs"
    },
    "./led": {
      "import": "./dist/led.js",
      "require": "./dist/led.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "fmt": "prettier -c -w .",
    "lint": "pnpm run /lint:.*/",
    "lint:fmt": "prettier -l .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cmake-js reconfigure",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ashnel3/logi-js.git"
  },
  "keywords": [
    "logitech",
    "glab"
  ],
  "author": "ashnel3",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashnel3/logi-js/issues"
  },
  "homepage": "https://github.com/ashnel3/logi-js#readme",
  "engineStrict": true,
  "engines": {
    "node": ">= 18"
  },
  "packageManager": "pnpm@9.8.0",
  "os": [
    "win32"
  ],
  "dependencies": {
    "bindings": "^1.5.0"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@types/node": "^22.7.4",
    "cmake-js": "^7.3.0",
    "husky": "^9.1.6",
    "node-addon-api": "^8.1.0",
    "prettier": "^3.3.3",
    "rollup-plugin-node-externals": "^7.1.3",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-node": "^2.1.1",
    "vite-plugin-dts": "^4.2.2",
    "vitest": "^2.1.1"
  }
}
